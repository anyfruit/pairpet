{
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["ES2017"],
    "types": ["miniprogram-api-typings"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "miniprogram_npm"]
}
